<Window x:Class="_20260218_WpfApp1.View.MaintenanceHistory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_20260218_WpfApp1"
        mc:Ignorable="d"
        Title="Maintenance" Height="720" Width="1280" WindowStartupLocation="CenterScreen" Foreground="White">
    <Grid Background="#FF672121">
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Content="CAR RENTAL SYSTEM" Foreground="WHITE" FontSize="15" Margin="10, 10, 10, 10"/>
        <Border Grid.Row="0" BorderBrush="White" BorderThickness="0,0,0,5"/>
        <StackPanel Grid.Row="1" VerticalAlignment="Center" Width="600">
            <TextBlock Text="MAINTENANCE HISTORY" Margin="25, 50, 25, 25" HorizontalAlignment="Center" FontSize="35" FontWeight="Bold"/>
            <TextBlock Text="This is where the cars will be presented."/>
        </StackPanel>
    </Grid>
</Window>
